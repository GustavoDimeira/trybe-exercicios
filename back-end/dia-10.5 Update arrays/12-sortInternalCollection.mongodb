// Adicione aos atores de cast do character Batman do filme Batman os valores "Michael Keaton", "Val Kilmer" e "George Clooney", e deixe o array em ordem alfab√©tica.

use("cinema");

db.movies.updateOne(
  {
    title: "Batman",
    "cast.character": "Batman"
  },
  {
    $addToSet: {
      "cast.$.actor": {
        $addToSet: [
          "Michael Keaton", "Val Kilmer" , "George Clooney"
        ],
        $sort: 1
      }
    }
  }
);

db.movies.findOne({title: "Batman"});
